{
    "name": "presprog/contao-password-suggestion",
    "description": "Adds an one click password generator to the backend user management of Contao Open Source CMS to simplify adding new back end users.",
    "keywords": ["contao", "user", "password"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Present Progressive",
            "email": "hello@presentprogressive.de",
            "homepage": "http://www.presentprogressive.de"
        }
    ],
    "require": {
        "contao/core-bundle": "^4.13"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PresProg\\PasswordSuggestion\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "PresProg\\PasswordSuggestion\\ContaoManager\\Plugin"
    },
    "support": {
        "issues": "https://github.com/presprog/contao-password-suggestion/issues"
    }
}
